# AWS Configuration
AWS_REGION=ap-southeast-1
AWS_ACCESS_KEY_ID=your_access_key
AWS_SECRET_ACCESS_KEY=your_secret_key

# Auth Service
AUTH_JWT_SECRET=your_jwt_secret_key
AUTH_TOKEN_EXPIRY=86400

# DynamoDB Tables
USERS_TABLE=careercanvas-auth-users-dev
