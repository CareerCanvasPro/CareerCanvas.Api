{
  "name": "@careercanvas-api/user-management",
  "version": "0.0.0",
  "description": "User management service for Career Canvas API",
  "author": "CareerCanvas Team <support@careercanvas.pro>",
  "homepage": "",
  "license": "ISC",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "tsc",
