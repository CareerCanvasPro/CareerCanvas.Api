{
  "name": "@careercanvas-api/auth",
  "version": "0.0.0",
  "description": "Authentication service for Career Canvas API",
  "author": "CareerCanvas Team <support@careercanvas.pro>",
  "homepage": "",
  "license": "ISC",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "deploy": "node lib/services/auth/src/server",
    "server": "concurrently \"npm run watch-tsc\" \"npm run watch-deploy\"",
    "start": "npm run deploy",
    "test": "echo \"Error: run tests from root\" && exit 1",
    "tsc": "tsc",
    "watch-deploy": "node --watch lib/services/auth/src/server",
    "dev": "node --watch lib/services/auth/src/server",
    "watch-tsc": "tsc --watch"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "3.485.0",
    "@aws-sdk/lib-dynamodb": "3.485.0",
    "@types/maxmind": "^2.0.5",
    "maxmind": "^4.3.18",
    "date-fns": "^3.6.0",
    "dynamodb-toolbox": "0.9.2",
    "cm-authenticator": "file:../../packages/auth-middleware/cm-authenticator-1.0.0.tgz"
  }
}
